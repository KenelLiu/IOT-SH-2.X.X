	<groupId>com.iot.sh</groupId>
	<artifactId>iot-parent</artifactId>
	<version>1.0.0</version>
	<packaging>pom</packaging>
		
	<name>IOT-Parent</name>
	<url>http://maven.apache.org</url>
	
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<java.version>1.8</java.version>
		<iot.sh.version>1.0.0</iot.sh.version>
		<!--  spring framework version -->
		<spring.framework.version>5.3.27</spring.framework.version>
		<spring.boot.version>2.7.12</spring.boot.version>
		<spring.cloud.version>2021.0.7</spring.cloud.version>
		<!-- mysql jdbc version-->
		<mysql.version>8.0.33</mysql.version>
		<mybatis.spring.boot.version>2.3.1</mybatis.spring.boot.version>
		<!-- json Serialization deserialization -->
		<!--<alibaba.fastjson.version>1.2.83</alibaba.fastjson.version>-->
