		<ftp.version>3.9.0</ftp.version>
		<sftp.version>0.1.55</sftp.version>
		<bcprov.version>1.70</bcprov.version>
		<!-- lombok version -->
		<lombok.version>1.18.24</lombok.version>
		<ognl.version>3.3.4</ognl.version>
		<io.jsonwebtoken.version>0.9.1</io.jsonwebtoken.version>
		<ws.schild.version>3.3.1</ws.schild.version>
		<spring.security.version>5.8.3</spring.security.version>
	</properties>
